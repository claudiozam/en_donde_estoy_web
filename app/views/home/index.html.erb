<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB0s54l4ACCfRvJHfk9RXK-gpFJyBRA0ZU&sensor=false"></script>

<% content_for :scripts do %>
	<script type="text/javascript">
		$(function() { 
			MapUtils.configMap('general_map');
		});
	</script>
<% end %>

<div class="row-fluid">
	<div class="span12">
<!--		<div id="map_filters">Bucador para el mapa....ccs... <a href="#" id="linkTest">Test</a> </div> -->
		<div id="map_filters">
			<form class="form-inline">
				<label class="control-label" for="selectedCategoria">Categoria: </label>
    			<select id="selectedCategoria">
					<option>Categoria 1</option>
					<option>Categoria 2</option>
					<option>Categoria 3</option>
				</select>
				<label class="control-label" for="selectedTipo">Tipo: </label>
    			<select id="selectedTipo">
					<option>Tipo A</option>
					<option>Tipo B</option>
					<option>Tipo C</option>
				</select>
				<label class="control-label" for="inputTexto">Texto: </label>
    			<input type="text" class="input-large" id="inputTexto" placeholder="Ingrese el texto deseado">
				<label class="control-label" for="selectedKM">KM> </label>
    			<select id="selectedKM">
					<option>KM 10</option>
					<option>KM 20</option>
					<option>KM 30</option>
				</select>
    			<button type="button" id="linkBuscar" class="btn">Buscar</button>
    		</form>
		</div>
	</div>
	<div class="row-fluid">
    <div class="span2">
      <div id="map_filters_result"></div>
    </div>
    <div class="span10">
      <div id="general_map"></div>
    </div>
</div>